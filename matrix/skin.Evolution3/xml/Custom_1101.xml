<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="1101">
	<zorder>2</zorder>
	<onload>alarmclock(timer_1101,action(select,1101),00:01,silent,loop)</onload>	
	<onunload>cancelalarm(timer_1101,silent)</onunload>
	<onunload>clearproperty(fin_generique,home)</onunload>	
	<onunload condition="!player.hasvideo">clearproperty(lecture_bande_annonce,home)</onunload>	
	<onunload condition="!player.hasvideo">clearproperty(clearlogo_bande_annonce,home)</onunload>
	<onunload condition="window.isactive(home) + !$EXP[gestion_librairie] + $EXP[menu_principal] + !player.hasvideo">alarmclock(focus_600,setfocus(800),00:00,silent)</onunload>	
	<onunload condition="window.isactive(home) + !$EXP[gestion_librairie] + !$EXP[categorie_masquee] + !player.hasvideo">alarmclock(focus_600,setfocus($INFO[window(home).property(categorie)]),00:00,silent)</onunload>			
	<onunload condition="window.isactive(home) + !$EXP[gestion_librairie] + $EXP[categorie_masquee] + !$EXP[menu_principal] + !player.hasvideo + $EXP[zoom_interface]">alarmclock(focus_600,setfocus(600),00:00,silent)</onunload>		
	<onunload condition="window.isactive(home) + !$EXP[gestion_librairie] + $EXP[categorie_masquee] + !$EXP[menu_principal] + $EXP[librairie_films] + !player.hasvideo + !$EXP[zoom_interface] + !window.isvisible(movieinformation)">alarmclock(focus_600,setfocus($INFO[window(home).property(liste_courante_films)]),00:00,silent)</onunload>
	<onunload condition="window.isactive(home) + !$EXP[gestion_librairie] + $EXP[categorie_masquee] + !$EXP[menu_principal] + $EXP[librairie_series] + !player.hasvideo + !$EXP[zoom_interface] + !window.isvisible(movieinformation)">alarmclock(focus_600,setfocus($INFO[window(home).property(liste_courante_series)]),00:00,silent)</onunload>
	<onunload condition="window.isactive(home) + !$EXP[gestion_librairie] + $EXP[categorie_masquee] + !$EXP[menu_principal] + $EXP[librairie_episodes] + !player.hasvideo + !$EXP[zoom_interface] + !window.isvisible(movieinformation)">alarmclock(focus_600,setfocus($INFO[window(home).property(liste_courante_episodes)]),00:00,silent)</onunload>
	<onunload condition="window.isactive(home) + $EXP[gestion_librairie] + !player.hasvideo">alarmclock(focus,setfocus($INFO[window(home).property(bouton_courant_gestion_librairie)]),00:00,silent)</onunload>
	<defaultcontrol always="true">700</defaultcontrol>	
	<visible>player.hasvideo</visible>			
<controls>
	<control type="button" id="700">
		<onclick condition="!player.hasvideo">dialog.close(1101,true)</onclick>	
	</control>
</controls>
</window>
